target 'Take' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'
  pod 'Geofirestore'
  pod 'SwiftLint'
  pod 'CodableFirebase'
  pod 'Firebase/Firestore'
  pod 'SwiftyDraw'
  pod 'Mapbox-iOS-SDK'
  pod 'Tabman', '~> 1.10'
  pod "Fuse"

end
